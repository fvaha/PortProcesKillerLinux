{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "PortKiller",
  "version": "1.0.2",
  "identifier": "com.portkiller.linux",
  "build": {
    "frontendDist": "../ui",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Port Killer",
        "width": 950,
        "height": 650,
        "resizable": true,
        "fullscreen": false,
        "transparent": true,
        "decorations": false,
        "shadow": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}